<Window x:Class="AppStart_OS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppStart_OS"
        mc:Ignorable="d"
        Title="MainWindow" Height="150" Width="400">
    <Grid>
        <Button x:Name="btnOpenApp" Content="Открыть" HorizontalAlignment="Left" Margin="10,58,0,0" VerticalAlignment="Top" Height="41" Width="84" Click="btnOpenApp_Click"/>
        <TextBox x:Name="tbAppName" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Название приложение" VerticalAlignment="Top" Width="313" Height="28"/>
    </Grid>
</Window>
